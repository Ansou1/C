#include "42.h"

int	op_plus(int a, int b)
{
  return (a + b);
}

int	op_moins(int a, int b)
{
  return (a - b);
}

int	op_div(int a, int b)
{
  if (b == 0)
    {
      fprintf(stderr, "Math error\n");
      return (0);
    }
  return (a / b);
}

int	op_mul(int a, int b)
{
  return (a * b);
}

int	op_mod(int a, int b)
{
  if (b == 0)
    {
      fprintf(stderr, "Math error\n");
      return (0);
    }
  return (a % b);
}
