#include "asm.h"

void	my_put_exit(char *str)
{
  my_putstr(str);
  exit(0);
}

int	my_max(int a, int b)
{
  if (a >= b)
    return (a);
  else
    return (b);
}

int	my_max3(int a, int b, int c)
{
  int	ret;

  if (a >= b)
    ret = a;
  else
    ret = b;
  if (ret >= c)
    return (ret);
  else
    return (c);
}
